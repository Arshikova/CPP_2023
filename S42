#include <iostream>
#include <algorithm>
using namespace std;

int main () {
    int m[] = {25, 8, 9, 11, 15, 23, 1, 7, 10};
    int size=9;
    for (int i=0; i<size;i++){
        int k=i;
        for(int j=i; j<size; j++)
            if (m[j]<m[k])
                k=j;
        swap (m[i], m[k]);
    }
    for (int i=0; i<size; i++){
        cout<< m[i]<< " ";
    }
    return 0;
}
