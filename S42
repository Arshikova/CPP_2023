#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    setlocale(0,"Russian");
    int m[9] = {25, 8, 9, 11, 15, 23, 1, 7, 10};
    sort(m, m + 9, greater<int>());
    cout << "Отсортированный массив ";
    for (int i = 0; i < 9; i++) {
        cout << m[i] << " ";
    }
    return 0;
}
