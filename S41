#include <iostream>
using namespace std;

float squared_sum(float a, float b){
    float c;
    c=(a+b)*(a+b);
    return c;
}

int main(){
    setlocale(0, "Russian");
    float a,b;
    cout<<"Введите число a: ";
    cin>>a;
    cout<<"Введите число b: ";
    cin>>b;
    cout <<"Квадрат суммы:"<< squared_sum(a,b) << endl;
}
