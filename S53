#include <iostream>
using namespace std;

void print(int a){
    cout << a << '\n';
}
void print(float a){
    cout << a << '\n';
}
void print(char *a){
    cout << a << '\n';
}
void print(int a, int b){
    cout << a << "," << b << endl;
}

int main(){
    setlocale(0, "Russian");
    float q=1.2545;
    print(54233), print(q), print("Книга"), print(45,485);
}
